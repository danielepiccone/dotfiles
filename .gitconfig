[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    dc = diff --cached
    lg = log -p
    lgn = log -p --name-only
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    lola = log --graph --decorate --pretty=oneline --abbrev-commit --all
    ls = ls-files  # Show files ignored by git:
    ign = ls-files -o -i --exclude-standard
    ff = merge --ff-only
    changes = log --pretty='%s'
    cp = cherry-pick -x
